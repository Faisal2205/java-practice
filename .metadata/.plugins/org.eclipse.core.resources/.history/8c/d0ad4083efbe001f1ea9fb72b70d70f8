package javaPractice;

public class Test {
	
	void add(int a)
	{
		System.out.println("normal");
	}
	 void add(int ...a)
	{
		System.out.println("var-arg");
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
         Test t= new Test();
		t.add(10);
		t.add(0);
		t.add(10,20,30);
		
	}

}
